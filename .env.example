# Cron Jobs
CRON_TZ=America/Sao_Paulo

# Rate limiting
THROTTLER_TTL=60
THROTTLER_LIMIT=10

# JWT
JWT_EXP=24h
JWT_SECRET=""

# SendGrid
SENDGRID_KEY=secret
SENDGRID_FROM=email